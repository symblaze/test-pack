{
  "name": "symblaze/test-pack",
  "description": "Symblaze test pack",
  "require": {
    "symfony/test-pack": "^1.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "symfony/framework-bundle": "^7.0"
  },
  "suggest": {
    "fakerphp/faker": "Required to use Faker with Symfony tests"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Symblaze\\TestPack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symblaze\\TestPack\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "imdhemy",
      "email": "imdhemy@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  }
}
