{
    "name": "symblaze/test-pack",
    "description": "Symblaze test pack",
    "require": {
        "symfony/test-pack": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Symblaze\\TestPack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symblaze\\TestPack\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "imdhemy",
            "email": "imdhemy@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    }
}
